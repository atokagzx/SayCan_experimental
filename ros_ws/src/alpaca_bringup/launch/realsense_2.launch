
<launch>
    <include file='$(find alpaca_bringup)/launch/camera_2_pose.launch' />

    <!--  launch realsense 2 -->
    <include file="$(find alpaca_bringup)/launch/realsense.launch" >
        <arg name="output"              value="screen"/>
        <arg name="filters"             value="decimation, disparity, spatial, temporal, disparity"/> 
        <arg name="publish_tf"          value="false"/>
        <arg name="tf_publish_rate"     value="0"/>
        <arg name="serial_no"           value="153122073918"/>
        <arg name="camera"              value="/sensor/camera/2"/>
        <arg name="tf_prefix"           value="camera_2"/>
        <arg name="depth_width"         value="640"/>
        <arg name="depth_height"        value="480"/>
        <arg name="color_width"         default="848"/>
        <arg name="color_height"        default="480"/>
    </include>
</launch>